lookup LamAlefIsol {
  sub lam-ar.init by lam-ar.init.Alef;
  sub alef-ar.fina by alef-ar.fina.Lam;
} LamAlefIsol;

lookup LamAlefFina {
  sub lam-ar.medi by lam-ar.medi.Alef;
  sub alef-ar.fina by alef-ar.fina.Lam;
} LamAlefFina;

feature rlig {
  lookupflag IgnoreMarks;
    sub lam-ar.init' lookup LamAlefIsol alef-ar.fina' lookup LamAlefIsol;
    sub lam-ar.medi' lookup LamAlefFina alef-ar.fina' lookup LamAlefFina;
} rlig;

lookup AboveHah {
  sub lam-ar.init by lam-ar.init.Hah;
  sub behDotless-ar.init by behDotless-ar.init.Hah;
  sub hah-ar.medi by hah-ar.init;
  sub hah-ar.fina by hah-ar;
} AboveHah;

feature rclt {
  lookupflag IgnoreMarks;
    sub [lam-ar.init behDotless-ar.init]' lookup AboveHah [hah-ar.medi hah-ar.fina]' lookup AboveHah;
    pos [lam-ar.init.Hah behDotless-ar.init.Hah]' <80 0 80 0> [hah-ar];

  @ar2DotBelow = [twodotshorizontalbelow-ar];
  lookupflag UseMarkFilteringSet @ar2DotBelow;
    sub behDotless-ar.init.Hah @ar2DotBelow' by twodotsverticalabove-ar.alt;
} rclt;

feature cv01 {
  sub alef-ar      by alef-ar.1;

  sub hah-ar.fina  by hah-ar.fina.1;
  sub hah-ar.medi  by hah-ar.medi.1;

  sub ain-ar.fina  by ain-ar.fina.1;
  sub ain-ar.medi  by ain-ar.medi.1;

  sub kaf-ar.init  by kaf-ar.init.1;
  sub kaf-ar.medi  by kaf-ar.medi.1;
  sub kaf-ar.fina  by kaf-ar.fina.1;

  sub meem-ar      by meem-ar.1;
  sub meem-ar.fina by meem-ar.fina.1;
  sub meem-ar.medi by meem-ar.medi.1;

  sub heh-ar       by heh-ar.1;
  sub heh-ar.fina  by heh-ar.fina.1;
  sub heh-ar.medi  by heh-ar.medi.1;
  sub heh-ar.init  by heh-ar.init.1;

  sub waw-ar.fina  by waw-ar.fina.1;

  sub alefMaksura-ar by alefMaksura-ar.1;
  sub alefMaksura-ar.fina by alefMaksura-ar.fina.1;
} cv01;

feature cv01 {
  sub [heh-ar.1 heh-ar.fina.1] twodotsverticalabove-ar' by twodotshorizontalabove-ar;
} cv01;

feature cv02 {
  sub alef-ar by alef-ar.2;

  sub hah-ar.init by hah-ar.init.2;
  sub hah-ar.medi by hah-ar.medi.2;

  sub meem-ar by meem-ar.2;
  sub meem-ar.fina by meem-ar.fina.2;

  sub heh-ar.medi by heh-ar.medi.2;
  sub heh-ar.init by heh-ar.init.2;
} cv02;

# Undo the rclt above
lookup ReverseAboveHah {
  sub lam-ar.init.Hah by lam-ar.init;
  sub behDotless-ar.init.Hah by behDotless-ar.init;
  sub hah-ar.init.2 by hah-ar.medi.2;
} ReverseAboveHah;

feature cv02 {
  lookupflag IgnoreMarks;
    sub [lam-ar.init.Hah behDotless-ar.init.Hah]' lookup ReverseAboveHah hah-ar.init.2' lookup ReverseAboveHah;
} cv02;

feature cv03 {
  sub alef-ar by alef-ar.3;

  sub heh-ar.medi by heh-ar.medi.3;
  sub heh-ar.init by heh-ar.init.3;
} cv03;

feature rclt {
  @ar23DotAbove = [twodotshorizontalabove-ar threedotsupabove-ar hamzaabove-ar tahabove-ar];
  @ar23DotBelow = [twodotshorizontalbelow-ar threedotsdownbelow-ar hamzabelow-ar];
  sub [behDotless-ar.init behDotless-ar.medi] @ar23DotAbove behDotless-ar.medi' @ar23DotAbove by behDotless-ar.medi.Wide;
  sub [behDotless-ar.init behDotless-ar.medi] @ar23DotBelow behDotless-ar.medi' @ar23DotBelow by behDotless-ar.medi.Wide;
} rclt;

feature rclt {
  @ar23DotBelow = [twodotshorizontalbelow-ar threedotsdownbelow-ar];
  lookupflag UseMarkFilteringSet @ar23DotBelow;
    pos [reh-ar reh-ar.fina]' <100 0 100 0> behDotless-ar.init @ar23DotBelow;
} rclt;
