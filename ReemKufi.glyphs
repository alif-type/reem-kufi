    sub yeh-farsi by alefMaksura-ar.fina;
    sub hehHamzaabove-ar by heh-ar.fina.1 hamzaabove-ar;
    sub keheh-ar by kaf-ar.fina.1;
    sub noonThreedotsabove-ar by noonghunna-ar.fina threedotsupabove-ar;
